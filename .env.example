# Google Analytics
# GA4のMeasurement ID（G-XXXXXXXXXXの形式）
NEXT_PUBLIC_GA_MEASUREMENT_ID=

# Google Search Console
# HTMLタグ検証で取得したcontent値
NEXT_PUBLIC_GOOGLE_SITE_VERIFICATION=

# メール送信設定（Gmail）
# GmailのSMTPサーバー
SMTP_HOST=smtp.gmail.com
# SMTPポート
SMTP_PORT=587
# STARTTLS使用（587の場合はfalse）
SMTP_SECURE=false
# Gmailアドレス
SMTP_USER=your-email@gmail.com
# Googleアプリパスワード（16文字）
SMTP_PASSWORD=xxxx xxxx xxxx xxxx
# 送信元メールアドレス
SMTP_FROM=your-email@gmail.com
# お問い合わせ受信先メールアドレス
CONTACT_EMAIL=recruit@fractal-group.co.jp
